
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model owner {
  OWNN_ID    String @id @default(uuid())
  OWNC_LNAME String
  OWNC_FNAME String
  OWNN_TEL   String
  OWNC_MAIL  String
  OWNC_MDP   String
}

model tenant {
  TENN_ID    String @id @default(uuid())
  TENC_LNAME String
  TENC_FNAME String
  TENN_TEL   String
  TENC_MAIL  String
  TENC_MDP   String
}
